{
  "expo": {
    "name": "dice-app",
    "slug": "notificaciones",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vergil-hamburguer.png",
    "scheme": "diceapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "ebdaf312-d819-4c0e-aef8-3c41f68a5a24"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vichigato.notificaciones",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#1a1a2e",
        "foregroundImage": "./assets/images/vergil-hamburguer.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/vergil-hamburguer.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vergil-hamburguer.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a2e",
          "dark": {
            "backgroundColor": "#0f0f0f"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/vergil-hamburguer.png",
          "color": "#bd93f9"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "vichigato"
  }
}
